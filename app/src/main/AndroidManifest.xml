<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypets2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddDogActivity" android:label="Perritos" />
        <activity android:name=".AddCatActivity" android:label="Michitos" />
        <activity android:name=".EditDogActivity" android:label="Perritos" />
        <activity android:name=".EditCatActivity" android:label="Michitos" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".DogsIndexActivity" android:label="Perritos" />
        <activity android:name=".CatsIndexActivity" android:label="Michitos" />
    </application>

</manifest>